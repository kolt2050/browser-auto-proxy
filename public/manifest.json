{
    "manifest_version": 3,
    "name": "Auto Proxy",
    "version": "1.0.0",
    "description": "Auto proxy for blocked sites",
    "permissions": [
        "proxy",
        "storage",
        "webRequest",
        "webRequestAuthProvider",
        "alarms"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}